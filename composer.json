{
  "name": "vpa/di",
  "description": "The simple DI implementation for PHP 8.x with using Attributes #[Injectable]",
  "keywords": [
    "php 8.x",
    "dependency",
    "injection",
    "Attributes",
    "Injectable"
  ],
  "license": "AGPL-3.0-or-later",
  "homepage": "https://github.com/zolll23/di",
  "authors": [
    {
      "name": "Andrey Pakhomov",
      "email": "andrey.pahomov@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^4.8"
  },
  "autoload": {
    "psr-4": {
      "VPA\\": "code/src/VPA"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VPA\\": "code/tests"
    }
  },
  "scripts": {
    "psalm": "vendor/bin/psalm --init",
    "test": "vendor/bin/phpunit --coverage-text"
  }
}